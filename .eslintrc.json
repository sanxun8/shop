{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true,
    "cypress": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "db": "writable"
  },
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "no-unused-vars": "off"
  }
}
